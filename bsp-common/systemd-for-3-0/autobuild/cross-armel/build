PYTHON_CFLAGS="-I/var/ab/cross-root/armel/usr/include/python2.7" PYTHON_LIBS="-lpython2.7" ./configure --host=$HOST --build=x86_64-unknown-linux-gnu \
      --localstatedir=/var \
      --sysconfdir=/etc \
      --disable-lz4 --disable-libgcrypt \
      --enable-compat-libs \
      --disable-audit --disable-gnuefi \
      --disable-ima \
      --disable-kdbus \
      --with-sysvinit-path= \
      --with-sysvrcnd-path= \
      --with-ntp-servers=www.pool.ntp.org --disable-libiptc --disable-seccomp --cache-file=config.cache --disable-qrencode --disable-microhttpd --disable-libcurl --disable-libidn --disable-dbus --disable-libcryptsetup --disable-introspection --without-python
make
make install DESTDIR=$PKGDIR 
