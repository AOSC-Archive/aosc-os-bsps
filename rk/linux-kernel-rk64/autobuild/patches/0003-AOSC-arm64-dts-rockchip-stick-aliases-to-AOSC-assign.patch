From 0e6dec24805d6d83ec76b0d999702ef2aee83af2 Mon Sep 17 00:00:00 2001
From: Icenowy Zheng <icenowy@aosc.io>
Date: Mon, 30 Nov 2020 22:05:28 +0800
Subject: [PATCH 3/7] AOSC: arm64: dts: rockchip: stick aliases to AOSC
 assignment

Signed-off-by: Icenowy Zheng <icenowy@aosc.io>
---
 arch/arm64/boot/dts/rockchip/rk3328.dtsi | 4 ++--
 arch/arm64/boot/dts/rockchip/rk3399.dtsi | 6 +++---
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328.dtsi b/arch/arm64/boot/dts/rockchip/rk3328.dtsi
index 063ed0adbec4..449a58a2ee62 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3328.dtsi
@@ -28,8 +28,8 @@ aliases {
 		i2c2 = &i2c2;
 		i2c3 = &i2c3;
 		mmc0 = &sdmmc;
-		mmc1 = &sdio;
-		mmc2 = &emmc;
+		mmc1 = &emmc;
+		mmc2 = &sdio;
 		ethernet0 = &gmac2io;
 		ethernet1 = &gmac2phy;
 	};
diff --git a/arch/arm64/boot/dts/rockchip/rk3399.dtsi b/arch/arm64/boot/dts/rockchip/rk3399.dtsi
index edbbf35fe19e..f7daa13ea5c2 100644
--- a/arch/arm64/boot/dts/rockchip/rk3399.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3399.dtsi
@@ -29,9 +29,9 @@ aliases {
 		i2c6 = &i2c6;
 		i2c7 = &i2c7;
 		i2c8 = &i2c8;
-		mmc0 = &sdio0;
-		mmc1 = &sdmmc;
-		mmc2 = &sdhci;
+		mmc0 = &sdmmc;
+		mmc1 = &sdhci;
+		mmc2 = &sdio0;
 		serial0 = &uart0;
 		serial1 = &uart1;
 		serial2 = &uart2;
-- 
2.30.2

