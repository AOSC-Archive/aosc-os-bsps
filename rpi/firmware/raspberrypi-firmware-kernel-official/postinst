rm -f /boot/kernel7.img
rm -f /boot/bcm2709-rpi-2-b.dtb
rm -f /boot/bcm2709-rpi-3-b.dtb

dpkg-divert --package rpikernelhack --remove --rename /boot/kernel7.img
dpkg-divert --package rpikernelhack --remove --rename /boot/bcm2709-rpi-2-b.dtb
dpkg-divert --package rpikernelhack --remove --rename /boot/bcm2709-rpi-3-b.dtb

rm -r /usr/share/rpikernelhack/overlays

sync
